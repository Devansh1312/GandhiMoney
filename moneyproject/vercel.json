{
    "builds": [
        {
            "src": "moneyproject/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12",
                "buildCommand": "bash build_files.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "moneyproject/wsgi.py"
        }
    ]
}
